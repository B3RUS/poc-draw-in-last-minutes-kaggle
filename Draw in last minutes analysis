    "goals_minutes = game_events[game_events['type']=='Goals']\n",
    "\n",
    "goals_minutes = goals_minutes.groupby(['minute','type']).size().reset_index(name='count')\n",
    "goals_minutes = goals_minutes.sort_values(by='count', ascending=False)\n",
    "\n",
    "total_goals = goals_minutes['count'].sum()\n",
    "\n",
    "\n",
    "goals_minutes['percentage'] = (goals_minutes['count'] / total_goals) * 100\n",
    "\n",
    "goals_minutes = goals_minutes.sort_values(by='percentage', ascending=False)\n",
