   "source": [
    "score_groupby_count = games.groupby(['aggregate','home_club_goals','away_club_goals']).size().reset_index(name='count')\n",
    "score_groupby_count = score_groupby_count.sort_values(by='count', ascending=False)\n",
    "\n",
    "total_matches = score_groupby_count['count'].sum()\n",
    "\n",
    "\n",
    "score_groupby_count['percentage'] = (score_groupby_count['count'] / total_matches) * 100\n",
    "\n",
    "score_groupby_count = score_groupby_count.sort_values(by='percentage', ascending=False)\n",
    "\n",
    "score_groupby_count"
   ]
  },
